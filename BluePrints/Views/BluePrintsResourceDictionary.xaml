                <dxe:ComboBoxEditSettings ItemsSource="{Binding LookUpPHASES.Entities}" IsTextEditable="False" DisplayMember="INTERNAL_NUM" ValueMember="GUID"/>
            </dxg:GridColumn.EditSettings>
        </dxg:GridColumn>
        <dxg:GridColumn FieldName="BASELINE_ITEM.GUID_AREA" Header="AREA" DataContext="{Binding DataContext, ElementName=userControl}">
            <dxg:GridColumn.EditSettings>
                <dxe:ComboBoxEditSettings ItemsSource="{Binding LookUpAREAS.Entities}" IsTextEditable="False" DisplayMember="INTERNAL_NUM" ValueMember="GUID"/>
            </dxg:GridColumn.EditSettings>
        </dxg:GridColumn>
        <dxg:GridColumn FieldName="BASELINE_ITEM.GUID_DOCTYPE" Header="DOCTYPE" DataContext="{Binding DataContext, ElementName=userControl}" >
            <dxg:GridColumn.EditSettings>
                <dxe:ComboBoxEditSettings ItemsSource="{Binding LookUpDOCTYPES.Entities}" IsTextEditable="False" DisplayMember="NAME" ValueMember="GUID"/>
            </dxg:GridColumn.EditSettings>
        </dxg:GridColumn>
        <dxg:GridColumn FieldName="BASELINE_ITEM.GUID_DISCIPLINE" Header="DISCIPLINE" DataContext="{Binding DataContext, ElementName=userControl}" >
            <dxg:GridColumn.EditSettings>
                <dxe:ComboBoxEditSettings ItemsSource="{Binding LookUpDISCIPLINES.Entities}" IsTextEditable="False" DisplayMember="NAME" ValueMember="GUID"/>
            </dxg:GridColumn.EditSettings>
        </dxg:GridColumn>
        <dxg:GridColumn FieldName="BASELINE_ITEM.GUID_DEPARTMENT" Header="DEPARTMENT" DataContext="{Binding DataContext, ElementName=userControl}" >
            <dxg:GridColumn.EditSettings>
                <dxe:ComboBoxEditSettings ItemsSource="{Binding LookUpDEPARTMENTS.Entities}" IsTextEditable="False" DisplayMember="NAME" ValueMember="GUID"/>
            </dxg:GridColumn.EditSettings>
        </dxg:GridColumn>
        <dxg:GridColumn FieldName="BASELINE_ITEM.INTERNAL_NUM" Header="INTERNAL_NUM" />
        <dxg:GridColumn FieldName="BASELINE_ITEM.CLIENT_NUM" Header="CLIENT_NUM" />
        <dxg:GridColumn FieldName="BASELINE_ITEM.REVISION_NUMBER" Header="REVISION_NUMBER" />
        <dxg:GridColumn FieldName="BASELINE_ITEM.DELIVERABLE_TYPE" Header="DELIVERABLE_TYPE" >
            <dxg:GridColumn.EditSettings>
                <dxe:ComboBoxEditSettings IsTextEditable="True" AutoComplete="True" ItemsSource="{dxe:EnumItemsSource EnumType={x:Type common:DeliverableType}}"/>
            </dxg:GridColumn.EditSettings>
