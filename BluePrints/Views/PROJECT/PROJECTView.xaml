                        <Grid dxb:MergingProperties.AllowMerging="False" x:Name="PROJECTVARIATIONSDetailsPanel" dxb:BarNameScope.IsScopeOwner="True" DataContext="{Binding PROJECTVARIATIONSDetails}" dxlc:LayoutControl.TabHeader="VARIATIONS">
