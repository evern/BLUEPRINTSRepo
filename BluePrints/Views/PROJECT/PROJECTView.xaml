                        </Grid>
                        <Grid dxb:MergingProperties.AllowMerging="False" x:Name="PROJECTCOMMODITIESDetailsPanel" dxb:BarNameScope.IsScopeOwner="True" DataContext="{Binding PROJECTCOMMODITIESDetails}" dxlc:LayoutControl.TabHeader="COMMODITIES">
